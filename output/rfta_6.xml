                            endTime="00:01:04">
            <pbcore:pbcoreIdentifier source="local">20190920_James_Zachary_Q1</pbcore:pbcoreIdentifier>
            <pbcore:pbcoreTitle>Could you state your name and where you're from?
</pbcore:pbcoreTitle>
            <pbcore:pbcoreDescription>Question 1</pbcore:pbcoreDescription>
         </pbcore:pbcorePart>
         <pbcore:pbcorePart partType="Interview Questions"
                            startTime="00:01:04"
