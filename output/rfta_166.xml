         </pbcore:pbcorePart>
         <pbcore:pbcorePart partType="geographic" startTime="00:01:49" endTime="00:01:53">
            <pbcore:pbcoreIdentifier source="local">2021_03_11_Henriquez_Billy_G2</pbcore:pbcoreIdentifier>
            <pbcore:pbcoreTitle>Tegucigalpa, Honduras</pbcore:pbcoreTitle>
            <pbcore:pbcoreDescription>Interviewee discusses Tegucigalpa, Honduras.</pbcore:pbcoreDescription>
         </pbcore:pbcorePart>
         <pbcore:pbcorePart partType="Preguntas de entrevista"
